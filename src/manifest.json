  {
    "name": "chrome-extension-tutorial",
    "version": "0.0.5",
    "description": "Change event background colors of rakumo calendar",
    "icons": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://a-rakumo.appspot.com/calendar*"],
        "js": ["js/content.js"]
      }
    ],
    "manifest_version": 2
  }
